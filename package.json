{
  "name": "react-image-annotate-mngo",
  "version": "0.8.4",
  "sideEffects": false,
  "private": false,
  "type": "module",
  "homepage": "./",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adityasuman2025/MNgoImageAnnotate.git"
  },
  "scripts": {
    "start": "concurrently \"npx tailwindcss -i ./src/tailwind.css -c ./tailwind.config.cjs -o ./src/lib/index.css --watch\" \"vite\" ",
    "build": "npx tailwindcss -i ./src/tailwind.css -c ./tailwind.config.cjs -o ./src/lib/index.css --minify && tsc && IS_DEPL=true vite build",
    "babel-build": "rm -rf dist && npx tailwindcss -i ./src/tailwind.css -c ./tailwind.config.cjs -o ./src/lib/index.css --minify && tsc && vite build",
    "build-publish": "npm run babel-build && npm publish",
    "demo": "cd demo && rm -rf node_modules && npm install && npm start"
  },
  "dependencies": {
    "html2canvas": "^1.4.1"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-css-injected-by-js": "^2.2.0"
  }
}
